###### New host file format #######
[routers:children]
routers_ssh_keys
routers_no_ssh_keys

[routers_ssh_keys]
R1 ansible_host=192.168.122.10 ansible_user=ansible_user 

[routers_ssh_keys:vars]
ansible_connection=network_cli
ansible_network_os=ios
ansible_ssh_private_key_file=/home/bharat/.ssh/id_rsa
ansible_become=yes
ansible_become_method=enable
ansible_become_pass=Nyk@@123#

[routers_no_ssh_keys]
R2 ansible_host=192.168.122.11 

[routers_no_ssh_keys:vars]
ansible_user=bharat 
ansible_password=Nyk@@123#
ansible_connection=network_cli
ansible_network_os=ios
ansible_become=yes
ansible_become_method=enable
ansible_become_pass=Nyk@@123#